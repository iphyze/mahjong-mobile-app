{
  "expo": {
    "name": "mahjong",
    "slug": "mahjong",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iphyze.mahjong"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "95bbb45e-1d9b-4fba-9679-f193bc2cf8d8"
      }
    },
    "owner": "iphyze"
  }
}
